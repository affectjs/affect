{
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/@affectjs/affect/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nx/vite:build",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "packages/@affectjs/affect/dist"
            }
        },
        "type-check": {
            "executor": "nx:run-commands",
            "options": {
                "command": "tsc --noEmit",
                "cwd": "packages/@affectjs/affect"
            }
        }
    }
}

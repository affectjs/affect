{
  "name": "@affectjs/affect",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/affect/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm build",
        "cwd": "{projectRoot}"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm type-check",
        "cwd": "{projectRoot}"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm clean",
        "cwd": "{projectRoot}"
      }
    }
  },
  "tags": ["cli", "tools"]
}

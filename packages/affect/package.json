{
  "name": "@affectjs/affect",
  "version": "6.1.1",
  "description": "AffectJS CLI - Easy way to run affect DSL and setup FFmpeg environment",
  "keywords": [
    "ffmpeg",
    "cli",
    "setup",
    "environment"
  ],
  "author": "Albert Li <albert_lee@hotmail.com.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "affect": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "dependencies": {
    "@affectjs/runtime-node": "workspace:*",
    "@affectjs/dsl": "workspace:*",
    "@affectjs/fluent-setup": "workspace:*",
    "commander": "^12.0.0",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "prettier": "^3.7.4",
    "typescript": "^5.3.0",
    "vite": "^5.1.0"
  }
}
